{
  "name": "umdlib",
  "version": "0.6.0",
  "description": "A template for writing micro UMD Javascript libraries",
  "main": "index.js",
  "bin": {
    "umdlib": "./bin/umdlib.js"
  },
  "scripts": {
    "build": "gulp build",
    "watch": "gulp watch",
    "makedist": "gulp",
    "makelib": "gulp makenoparentlib",
    "test": "cross-env NODE_ENV=test nyc --reporter=lcov _mocha ./test/main.js",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "display-coverage": "opener ./coverage/lcov-report/index.html",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "report": "nyc report",
    "precommit": "npm run makedist && npm run test && npm run check-coverage && npm run report"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jclo/umdlib.git"
  },
  "keywords": [
    "UMD",
    "library",
    "template",
    "boilerplate",
    "instantiation patterns",
    "functional instantiation",
    "functional-shared instantiation",
    "prototypal instantiation",
    "pseudoclassical instantiation",
    "pseudoclassical auto instantiation"
  ],
  "author": {
    "name": "jclo",
    "email": "jclo@mobilabs.fr",
    "url": "http://www.mobilabs.fr/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jclo/umdlib/issues"
  },
  "homepage": "https://github.com/jclo/umdlib",
  "dependencies": {
    "nopt": "4.0.1",
    "path": "0.12.7"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "coveralls": "3.0.0",
    "cross-env": "5.1.3",
    "del": "3.0.0",
    "eslint": "4.16.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.8.0",
    "gulp": "3.9.1",
    "gulp-concat": "2.6.1",
    "gulp-footer": "2.0.1",
    "gulp-header": "2.0.1",
    "gulp-replace": "0.6.1",
    "gulp-uglify": "3.0.0",
    "husky": "0.14.3",
    "mocha": "5.0.0",
    "nyc": "11.4.1",
    "opener": "1.4.3",
    "require-dir": "0.3.2",
    "run-sequence": "2.2.1"
  }
}
